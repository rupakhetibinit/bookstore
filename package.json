{
	"private": true,
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"start": "next start",
		"migrate": "yarn prisma migrate dev",
		"format": "prettier --check --ignore-path .gitignore .",
		"format:fix": "prettier --write --ignore-path .gitignore .",
		"dev:turbo": "concurrently \"next dev --turbo\" \"tailwindcss --input ./styles/global.css --output ./styles/output.css --watch\"",
		"build:turbo": "tailwindcss input.css --output output.css && next build"
	},
	"dependencies": {
		"@lucia-auth/adapter-prisma": "^0.1.4",
		"@lucia-auth/nextjs": "^0.4.0",
		"@prisma/client": "^4.7.1",
		"clsx": "^1.2.1",
		"concurrently": "^7.6.0",
		"eslint-config-next": "latest",
		"lucia-auth": "^0.3.1",
		"next": "canary",
		"react": "latest",
		"react-dom": "latest",
		"react-hydration-provider": "^1.0.1",
		"react-responsive": "^9.0.2"
	},
	"devDependencies": {
		"@tailwindcss/forms": "^0.5.3",
		"@tailwindcss/typography": "^0.5.8",
		"@types/node": "18.11.3",
		"@types/react": "18.0.21",
		"@types/react-dom": "18.0.6",
		"autoprefixer": "^10.4.13",
		"postcss": "^8.4.18",
		"prettier": "^2.8.1",
		"prisma": "^4.7.1",
		"tailwindcss": "^3.2.4",
		"typescript": "4.8.4"
	},
	"packageManager": "yarn@3.3.0"
}
